                    xmlns:System="clr-namespace:System;assembly=mscorlib">

    <FontFamily x:Key="MainFont">..\Resources\Fonts\Raleway-Regular.ttf#Raleway</FontFamily>
    <FontFamily x:Key="AccentFont">..\Resources\Fonts\NotoSerif-Regular.ttf#Noto Serif</FontFamily>

    <System:Double x:Key="RegularFontSize">14</System:Double>
